{
  "name": "game-ngrx",
  "version": "0.0.0",
  "scripts": {
    "gen-pwa": "ng build --prod && cd dist/game-ngrx && http-server -p 3456",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serv": "cd json-server && json-server --watch db.json",
    "waves-locale": "sudo docker run -d -p 6869:6869 wavesplatform/waves-private-node",
    "explorer": "sudo docker run -d -e API_NODE_URL=http://localhost:6869 -e NODE_LIST=http://localhost:6869 -p 3333:80 wavesplatform/explorer",
    "issue-tokens-transfer-to-gamers-deploy-contract": "npx surfboard test issue-token-test.js",
    "tn": "npx surfboard test --env=testnet",
    "docker-ports": "ps aux | grep docker",
    "check-docker-port": "netstat -ant  |grep 2376",
    "18ru": "npx ng xi18n --i18n-locale ru",
    "18zh": "npx ng xi18n --i18n-locale zh",
    "test-guessNumber": "npx surfboard test guess-number-test.js",
    "build-i18n": "for lang in en; do ng build --output-path=dist/$lang --aot --prod --base-href /$lang/ --i18n-file=src/app/localize/messages.$lang.xlf --i18n-format=xlf --i18n-locale=$lang; done",
    "check-free-port": "sudo nc localhost 56733 < /dev/null; echo $?"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.11",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "~9.1.11",
    "@angular/compiler": "~9.1.11",
    "@angular/core": "~9.1.11",
    "@angular/forms": "~9.1.11",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "~9.1.11",
    "@angular/platform-browser-dynamic": "~9.1.11",
    "@angular/router": "~9.1.11",
    "@angular/service-worker": "~9.1.11",
    "@ngmodule/material-carousel": "^0.6.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.8",
    "@angular/cli": "~9.1.8",
    "@angular/compiler-cli": "~9.1.11",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@waves/surfboard": "^1.11.7",
    "codelyzer": "^5.1.2",
    "dotenv": "^8.2.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~7.0.0",
    "replace-in-file": "^6.1.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  }
}
